{
  "id": "nucleo_f091rc",
  "name": "NUCLEO-F091RC",
  "pack": "stm32f0",
  "mcu": "STM32F091RC",
  "memory": {
    "flash": {
      "origin": "0x08000000",
      "length": "0x00040000"
    },
    "ram": {
      "origin": "0x20000000",
      "length": "0x00008000"
    }
  },
  "led": {
    "name": "LD2",
    "port": "A",
    "pin": 5,
    "active_high": true
  },
  "reserved_pins": [
    { "port": "A", "pin": 13, "reason": "SWDIO" },
    { "port": "A", "pin": 14, "reason": "SWCLK" }
  ],
  "named_pins": [
    { "name": "LD2", "port": "A", "pin": 5, "mode": "output", "initial": "low", "active_high": true },
    { "name": "B1", "port": "C", "pin": 13, "mode": "input", "pull": "none" }
  ],
  "gpio_ports": ["A", "B", "C"],
  "serial": {
    "usart": "USART2",
    "baud": 115200,
    "tx": { "port": "A", "pin": 2, "af": 1 },
    "rx": { "port": "A", "pin": 3, "af": 1 }
  },
  "openocd": {
    "interface_cfg": "interface/stlink.cfg",
    "transport": "auto",
    "speed_khz": 4000,
    "reset_config": [
      "reset_config srst_only srst_nogate connect_assert_srst"
    ]
  }
}
