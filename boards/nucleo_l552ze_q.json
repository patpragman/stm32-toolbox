  "pack": "stm32l5",
  "mcu": "STM32L552ZE",
  "memory": {
    "flash": {
      "origin": "0x08000000",
      "length": "0x00080000"
    },
    "ram": {
      "origin": "0x20000000",
      "length": "0x00040000"
    }
  },
  "led": {
    "name": "LD2",
    "port": "B",
    "pin": 7,
    "active_high": true
